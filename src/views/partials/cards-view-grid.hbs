<div class="web-cards primary-content">
    {{#each factionCards as |fac|}}
        <div class="faction-{{fac.faction_slug}}">
            <div class="container">
                <h2 class="faction-heading">{{> svg-icon icon=fac.faction_svg_sprite}}{{fac.faction_name}}</h2>
            </div>
            <div class="faction-cards">
                {{#each cards as |singleCardData|}}
                    {{> (lookup singleCardData 'template') singleCardData}}
                {{/each}}
            </div>
        </div>
    {{/each}}
</div>