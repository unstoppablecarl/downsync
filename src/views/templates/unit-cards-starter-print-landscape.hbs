{{> header title="Starter Unit Cards Print Landscape"}}
<body class="page-preview-background">
<style>
    @media print {
        @page {
            size: landscape
        }
    }
</style>
<div class="no-print">
    {{> nav}}
</div>

<div class="output-container card-grid-container-flex-landscape-print">
    {{#each cardPages as |cardPage|}}

        <div class="page-preview page-letter-landscape" style="background-color:white">
            <div class="page-card-grid">
                {{#each cardPage as |cardData|}}

                    {{> (lookup cardData 'template') cardData}}
                {{/each}}
            </div>
        </div>
    {{/each}}
</div>

{{> footer }}
</body>
{{> close}}

